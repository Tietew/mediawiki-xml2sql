%{
enum element {
  el_mediawiki,
  el_siteinfo,
  el_sitename,
  el_base,
  el_generator,
  el_case,
  el_namespaces,
  el_namespace,
  el_page,
  el_title,
  el_id,
  el_restrictions,
  el_revision,
  el_timestamp,
  el_contributor,
  el_username,
  el_ip,
  el_minor,
  el_comment,
  el_text,
};
%}
struct eltmap { char *name; enum element t; };
%%
mediawiki,    el_mediawiki
siteinfo,     el_siteinfo
